{
  "name": "checkin-system-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "npm run husky-install",
    "husky-install": "cd .. && husky install frontend/.husky",
    "lint": "gts lint",
    "lint:fix": "gts fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8080",
  "devDependencies": {
    "@types/node": "^16.11.24",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gts": "^3.1.0",
    "husky": "^7.0.0",
    "prettier": "2.5.1",
    "pretty-quick": "^3.1.3",
    "webpack": "^5.69.1"
  },
  "dependencies": {
    "@chakra-ui/button": "^2.0.8",
    "@chakra-ui/icons": "^2.0.8",
    "@chakra-ui/react": "^2.2.8",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@uselessdev/datepicker": "^2.5.1",
    "axios": "^0.27.2",
    "date-fns": "^2.29.2",
    "date-fns-tz": "^1.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.54.4",
    "source-sans": "^3.46.0",
    "styled-components": "^5.3.5",
    "swr": "^1.3.0",
    "typescript": "^4.5.5",
    "websocket-ts": "^1.1.1"
  }
}
